		<component>
			<role>
				org.apache.maven.lifecycle.mapping.LifecycleMapping
			</role>
			<role-hint>eclipse-plugin</role-hint>
			<implementation>
				org.apache.maven.lifecycle.mapping.DefaultLifecycleMapping
			</implementation>
			<configuration>
				<phases>
				    <validate>
				    	org.codehaus.tycho:maven-osgi-packaging-plugin:build-qualifier
				    </validate>
					<process-resources>
						org.apache.maven.plugins:maven-resources-plugin:resources
					</process-resources>
					<compile>
						org.codehaus.tycho:maven-osgi-compiler-plugin:compile
					</compile>
					<process-test-resources>
						org.apache.maven.plugins:maven-resources-plugin:testResources
					</process-test-resources>
					<test-compile><!-- tests are always in a separate plugin --></test-compile>
					<test><!-- tests are always in a separate plugin --></test>
					<package>
						org.codehaus.tycho:maven-osgi-packaging-plugin:package-plugin
					</package>
					<integration-test></integration-test>
					<install>
						org.apache.maven.plugins:maven-install-plugin:install
					</install>
					<deploy>
						org.apache.maven.plugins:maven-deploy-plugin:deploy
					</deploy>
				</phases>
			</configuration>
		</component>
		<component>
			<role>
				org.apache.maven.artifact.handler.ArtifactHandler
			</role>
			<role-hint>eclipse-plugin</role-hint>
			<implementation>
				org.apache.maven.artifact.handler.DefaultArtifactHandler
			</implementation>
			<configuration>
				<classifier></classifier>
				<extension>jar</extension>
				<type>osgi-bundle</type>
				<packaging>osgi-bundle</packaging>
				<language>java</language>
				<addedToClasspath>true</addedToClasspath>
				<includesDependencies>false</includesDependencies>
			</configuration>
		</component>
		<component>
			<role>
				org.apache.maven.lifecycle.mapping.LifecycleMapping
			</role>
			<role-hint>eclipse-test-plugin</role-hint>
			<implementation>
				org.apache.maven.lifecycle.mapping.DefaultLifecycleMapping
			</implementation>
			<configuration>
				<phases>
				    <validate>
				    	org.codehaus.tycho:maven-osgi-packaging-plugin:build-qualifier
				    </validate>
					<process-resources>
						org.apache.maven.plugins:maven-resources-plugin:resources
					</process-resources>
					<compile>
						org.codehaus.tycho:maven-osgi-compiler-plugin:compile
					</compile>
					<process-test-resources>
						org.apache.maven.plugins:maven-resources-plugin:testResources
					</process-test-resources>
					<test-compile>
						org.codehaus.tycho:maven-osgi-compiler-plugin:testCompile
					</test-compile>
					<test></test>
					<package>
					</package>
					<integration-test>
						org.codehaus.tycho:maven-osgi-test-plugin:test
					</integration-test>
					<install>
					</install>
					<deploy>
					</deploy>
				</phases>
			</configuration>
		</component>
		<component>
			<role>
				org.apache.maven.lifecycle.mapping.LifecycleMapping
			</role>
			<role-hint>eclipse-feature</role-hint>
			<implementation>
				org.apache.maven.lifecycle.mapping.DefaultLifecycleMapping
			</implementation>
			<configuration>
				<phases>
				    <validate>
				    	org.codehaus.tycho:maven-osgi-packaging-plugin:build-qualifier
				    </validate>
					<generate-resources>
						org.codehaus.tycho:maven-osgi-packaging-plugin:copy-feature-files
					</generate-resources>
					<package>
						org.codehaus.tycho:maven-osgi-packaging-plugin:package-feature
					</package>
					<install>
						org.apache.maven.plugins:maven-install-plugin:install
					</install>
					<deploy>
						org.apache.maven.plugins:maven-deploy-plugin:deploy
					</deploy>
				</phases>
			</configuration>
		</component>
		<component>
			<role>
				org.apache.maven.artifact.handler.ArtifactHandler
			</role>
			<role-hint>eclipse-feature</role-hint>
			<implementation>
				org.apache.maven.artifact.handler.DefaultArtifactHandler
			</implementation>
			<configuration>
				<classifier></classifier>
				<extension>jar</extension>
				<type>eclipse-feature</type>
				<packaging>eclipse-feature</packaging>
				<language>java</language>
				<addedToClasspath>false</addedToClasspath>
				<includesDependencies>false</includesDependencies>
			</configuration>
		</component>




        <component>
            <role>
                org.apache.maven.artifact.handler.ArtifactHandler
            </role>
            <role-hint>eclipse-update-site</role-hint>
            <implementation>
                org.apache.maven.artifact.handler.DefaultArtifactHandler
            </implementation>
            <configuration>
                <classifier></classifier>
                <extension>jar</extension>
                <type>eclipse-update-site</type>
                <packaging>eclipse-update-site</packaging>
                <language>java</language>
                <addedToClasspath>false</addedToClasspath>
                <includesDependencies>false</includesDependencies>
            </configuration>
        </component>
        <component>
            <role>
                org.apache.maven.lifecycle.mapping.LifecycleMapping
            </role>
            <role-hint>eclipse-update-site</role-hint>
            <implementation>
                org.apache.maven.lifecycle.mapping.DefaultLifecycleMapping
            </implementation>
            <configuration>
                <phases>
                    <package>
                        org.codehaus.tycho:maven-osgi-packaging-plugin:update-site,
                        org.codehaus.tycho:maven-osgi-packaging-plugin:p2-metadata
                    </package>
                    <install>
                        org.apache.maven.plugins:maven-install-plugin:install
                    </install>
                    <deploy>
                        org.apache.maven.plugins:maven-deploy-plugin:deploy
                    </deploy>
                </phases>
            </configuration>
        </component>



        <component>
            <role>
                org.apache.maven.artifact.handler.ArtifactHandler
            </role>
            <role-hint>eclipse-application</role-hint>
            <implementation>
                org.apache.maven.artifact.handler.DefaultArtifactHandler
            </implementation>
            <configuration>
                <classifier></classifier>
                <extension>zip</extension>
                <type>eclipse-application</type>
                <packaging>eclipse-application</packaging>
                <language>java</language>
                <addedToClasspath>false</addedToClasspath>
                <includesDependencies>false</includesDependencies>
            </configuration>
        </component>
        <component>
            <role>
                org.apache.maven.lifecycle.mapping.LifecycleMapping
            </role>
            <role-hint>eclipse-application</role-hint>
            <implementation>
                org.apache.maven.lifecycle.mapping.DefaultLifecycleMapping
            </implementation>
            <configuration>
                <phases>
                    <package>
                        org.codehaus.tycho:maven-osgi-packaging-plugin:product-export
                    </package>
                    <install>
                        org.apache.maven.plugins:maven-install-plugin:install
                    </install>
                    <deploy>
                        org.apache.maven.plugins:maven-deploy-plugin:deploy
                    </deploy>
                </phases>
            </configuration>
        </component>

	</components>
</component-set>
