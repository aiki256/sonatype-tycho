<?xml version="1.0" encoding="UTF-8"?>
<project>
  <modelVersion>4.0.0</modelVersion>
  <parent>
    <artifactId>eclipse32</artifactId>
    <groupId>tycho109</groupId>
    <version>0.0.1-SNAPSHOT</version>
  </parent>
  <groupId>tycho109</groupId>
  <artifactId>HeadlessFeature</artifactId>
  <version>1.0.0</version>
  <packaging>eclipse-feature</packaging>

    <build>
        <plugins>
            <plugin>
                <groupId>org.codehaus.tycho</groupId>
                <artifactId>maven-osgi-packaging-plugin
                </artifactId>
                <version>0.3.0-SNAPSHOT</version>
                <executions>
                    <execution>
                        <phase>package</phase>
                        <goals>
                            <goal>product-export</goal>
                        </goals>
                        <configuration>
                            <productConfigurationFile>
                                HeadlessFeature.product
                            </productConfigurationFile>
                        </configuration>
                    </execution>
                </executions>
            </plugin>
        </plugins>
    </build>

</project>
